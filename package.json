{
	"name": "@fermion-app/sandbox",
	"version": "0.1.4",
	"description": "Secure isolated code execution sandbox SDK",
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"require": "./dist/index.js",
			"import": "./dist/index.mjs"
		}
	},
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"build": "tsx src/build.ts",
		"test": "tsx src/example.ts",
		"typecheck": "tsc --noEmit",
		"lint": "tsc && eslint . --ext .ts",
		"lint:fix": "eslint . --ext .ts --fix",
		"clean": "rm -rf dist",
		"pretty": "prettier --write \"**/*.{ts,tsx,json,yml,yaml}\" --log-level warn"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/fermion-app/sandbox.git"
	},
	"bugs": {
		"url": "https://github.com/fermion-app/sandbox/issues"
	},
	"homepage": "https://docs.fermion.app/",
	"files": [
		"dist",
		"README.md",
		"LICENSE"
	],
	"keywords": [
		"code-execution",
		"sandbox",
		"fermion",
		"container"
	],
	"license": "MIT",
	"dependencies": {
		"nanoid": "3.3.6",
		"ws": "8.18.3",
		"zod": "4.1.12"
	},
	"devDependencies": {
		"@types/node": "24.9.2",
		"@types/ws": "8.18.1",
		"@typescript-eslint/eslint-plugin": "8.46.2",
		"@typescript-eslint/parser": "8.46.2",
		"eslint": "9.38.0",
		"eslint-config-prettier": "10.1.8",
		"eslint-plugin-prettier": "5.5.4",
		"prettier": "3.6.2",
		"tsup": "8.5.0",
		"tsx": "4.20.6",
		"typescript": "5.9.3"
	}
}
